{
  "name": "static-by-nextjs",
  "version": "1.0.0",
  "description": "Static web page files generated by next.js",
  "engines": {
    "node": ">=13.8.0",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "start": "npm-run-all -s build dev:export dev:launch",
    "dev": "npm-run-all -p watch:* next-dev",
    "build": "next build",
    "next-dev": "next dev",
    "dev:export": "next export",
    "dev:launch": "browser-sync 'out' -w",
    "lint:eslint": "eslint --fix pages",
    "watch:eslint": "chokidar '**/*.tsx' '**/*.ts' -c 'eslint --fix {path}' "
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kkkaoru/static-by-nextjs.git"
  },
  "keywords": [],
  "author": "https://github.com/kkkaoru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkkaoru/static-by-nextjs/issues"
  },
  "homepage": "https://github.com/kkkaoru/static-by-nextjs#readme",
  "dependencies": {
    "eslint": "^7.8.1",
    "next": "^9.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.2",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/styled-components": "^5.1.3",
    "@types/stylelint": "^9.10.1",
    "@types/vfile-message": "2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "browser-sync": "^2.26.12",
    "chokidar-cli": "^2.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --max-warnings=0 --fix",
      "git add"
    ]
  }
}
